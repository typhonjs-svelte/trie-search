{
  "name": "@typhonjs-svelte/trie-search",
  "version": "0.0.0",
  "description": "Provides a trie-search and hash array data structures w/ Svelte support.",
  "license": "MPL-2.0",
  "type": "module",
  "author": "Michael Leahy <support@typhonjs.io> (https://github.com/typhonrt)",
  "contributors": [
    "Michael Leahy <support@typhonjs.io> (https://github.com/typhonrt)"
  ],
  "sideEffects": false,
  "imports": {
    "#runtime/data/struct/hash/array": "./src/hash/index.ts",
    "#runtime/data/struct/search/trie": "./src/trie/index.ts",
    "#runtime/*": "@typhonjs-svelte/runtime-base/*",
    "#svelte/store": "svelte/store"
  },
  "exports": {
    ".": {
      "types": "./dist/trie/index.d.mts",
      "import": "./dist/trie/index.js"
    },
    "./hash": {
      "types": "./dist/hash/index.d.mts",
      "import": "./dist/hash/index.js"
    },
    "./query": {
      "types": "./dist/query/index.d.mts",
      "import": "./dist/query/index.js"
    }
  },
  "devDependencies": {
    "@rollup/plugin-typescript": "^11.1.1",
    "@typhonjs-svelte/runtime-base": "github:typhonjs-svelte/runtime-base",
    "@vitest/coverage-c8": "^0.31.1",
    "@vitest/ui": "^0.31.1",
    "rollup": "^3.23.0",
    "rollup-plugin-dts": "^5.3.0",
    "svelte": "^3.59.1",
    "trie-search": "^1.4.1",
    "tslib": "^2.5.2",
    "typescript" : "^5.0.4",
    "vitest": "^0.31.1"
  },
  "files": [
    "/dist",
    "/dist-trl"
  ],
  "browserslist": [">5%", "not IE 11"],
  "scripts": {
    "rollup": "rollup -c",
    "eslint": "eslint .",
    "prepublishOnly": "npm run eslint && npm run build && npm run test",
    "test": "vitest --coverage",
    "test-ui": "vitest --ui --coverage",
    "coverage": "vitest run --coverage",
    "benchmark": "node --expose-gc ./node_modules/vitest/vitest.mjs bench"
  }
}
